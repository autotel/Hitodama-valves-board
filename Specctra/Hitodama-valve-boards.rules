
(rules PCB Hitodama-valve-boards
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 1425)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.0)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.0)
    )
  )
  (rule
    (width 500.0)
    (clear 310.2)
    (clear 250.0 (type smd_to_turn_gap))
    (clear 77.6 (type smd_smd))
  )
  (padstack "Via[0-1]_2000:1000_um"
    (shape
      (circle F.Cu 2000.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 2000.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_2000:1000_um" "Via[0-1]_2000:1000_um" default
  )
  (via 
    "Via[0-1]_2000:1000_um-kicad_default" "Via[0-1]_2000:1000_um" "kicad_default"
  )
  (via 
    "Via[0-1]_2000:1000_um-Power" "Via[0-1]_2000:1000_um" Power
  )
  (via_rule
    default "Via[0-1]_2000:1000_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_2000:1000_um-kicad_default"
  )
  (via_rule
    Power "Via[0-1]_2000:1000_um-Power"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 500.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    VCC "Net-(R1-Pad2)" "Net-(R2-Pad1)" "Net-(R3-Pad2)" "Net-(R3-Pad1)" "Net-(R5-Pad2)" "Net-(R6-Pad1)" "Net-(R7-Pad1)"
    "Net-(R7-Pad2)" "Net-(U2-Pad3)" "Net-(U3-Pad3)" "Net-(U5-Pad8)" /stby /A.IN.1 /B.IN.1 /B.PWM
    /A.PWM /B.PRESSURE /A.PRESSURE /A.IN.2 /B.IN.2
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 500.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class Power
    GND +12V /HVA+ "/HVA-" "/HVB-" /HVB+
    (clearance_class Power)
    (via_rule Power)
    (rule
      (width 1000.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)